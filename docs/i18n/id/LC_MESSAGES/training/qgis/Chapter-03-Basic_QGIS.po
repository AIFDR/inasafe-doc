#: ../../source/training/qgis/Chapter-03-Basic_QGIS.rst:199
msgid "Advanced Digitizing"
msgstr ""

# 062ec82dfddb402a99f9ce3a702193d7
#: ../../source/training/qgis/Chapter-03-Basic_QGIS.rst:201
msgid "Database"
msgstr ""

# 975ea768ab5f433b95bd13a5d8211078
#: ../../source/training/qgis/Chapter-03-Basic_QGIS.rst:203
msgid "GRASS"
msgstr ""

# 359c715355dd4df9a25ca2d28cbbe351
