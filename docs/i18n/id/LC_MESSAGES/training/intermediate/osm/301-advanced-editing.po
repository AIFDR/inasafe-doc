msgid "Chooses the Zoom tool"
msgstr ""

#: ../../source/training/intermediate/osm/301-advanced-editing.rst:432
# 9998beefbdda4c02b03df60e245d7285
msgid "Zoom in"
msgstr ""

#: ../../source/training/intermediate/osm/301-advanced-editing.rst:434
